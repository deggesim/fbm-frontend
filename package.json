{
  "name": "fbm-frontend",
  "version": "3.0.0",
  "description": "Fantabasket Manager",
  "author": "Simone De Gennaro",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deggesim/fbm-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/deggesim/fbm-frontend/issues"
  },
  "homepage": "https://github.com/deggesim/fbm-frontend#readme",
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production --sourceMap=false",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "format:fix": "pretty-quick --staged",
    "precommit": "run-s format:fix lint",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.json,.css,.scss,.html}\"",
    "prettier": "prettier --config ./.prettierrc \"src/{app,environments,assets,styles}/**/*{.json,.css,.scss,.html,.ts}\" --write"
  },
  "dependencies": {
    "@angular/animations": "~12.2.15",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "~12.2.15",
    "@angular/compiler": "~12.2.15",
    "@angular/core": "~12.2.15",
    "@angular/flex-layout": "^12.0.0-beta.35",
    "@angular/forms": "~12.2.15",
    "@angular/platform-browser": "~12.2.15",
    "@angular/platform-browser-dynamic": "~12.2.15",
    "@angular/router": "~12.2.15",
    "@fortawesome/angular-fontawesome": "^0.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@ng-select/ng-select": "^6.1.0",
    "@ngrx/effects": "^12.5.1",
    "@ngrx/router-store": "^12.5.1",
    "@ngrx/store": "^12.5.1",
    "@ngrx/store-devtools": "^12.5.1",
    "@swimlane/ngx-charts": "^17.0.1",
    "bootstrap": "^4.6.0",
    "bootswatch": "^4.6.0",
    "jquery": "^3.6.0",
    "jwt-decode": "^3.1.2",
    "koa": "^2.13.1",
    "koa-send": "^5.0.1",
    "koa-static": "^5.0.0",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.1",
    "ngx-bootstrap": "^6.2.0",
    "ngx-device-detector": "^2.0.6",
    "ngx-toastr": "^13.2.0",
    "popper.js": "^1.16.1",
    "rxjs": "~6.6.6",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.14",
    "@angular/cli": "~12.2.14",
    "@angular/compiler-cli": "~12.2.15",
    "@angular/language-service": "~12.2.15",
    "@types/jasmine": "~3.6.6",
    "@types/jasminewd2": "~2.0.8",
    "@types/jwt-decode": "^3.1.0",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "~14.14.33",
    "codelyzer": "^6.0.1",
    "husky": "^5.1.3",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "typescript": "~4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "tslint --fix"
    ],
    "*{.json,.css,.scss,.html,.js}": [
      "prettier --write"
    ]
  }
}
